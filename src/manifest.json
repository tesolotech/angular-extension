{
  "name": "Great Extension",
  "version": "1.0",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs"
],
"background": {
    "scripts": [
        "background-es2015.js",
        "runtime-es2015.js"
    ]
},
  "page_action": {
    "default_popup": "index.html"
  }
}
